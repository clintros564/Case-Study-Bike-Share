SELECT 
        ride_id, 
        rideable_type, 
        started_at, 
        ended_at, 
	  start_station_name, 
	  start_station_id, 
        end_station_name, 
	  end_station_id, 
        start_lat, 
        start_lng, 
	  end_lat, 
        end_lng, 
        member_casual,
	  ride_length,
	  CAST(started_at_time AS STRING) AS started_at_time,
	  ride_date,
	  ride_month,
	  day_of_week,
	  season
FROM 
        `careful-emitter-376816.CS_Bike_Share.202201`
UNION DISTINCT
